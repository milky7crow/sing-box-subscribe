{
  "log": {
    "level": "warn",
    "timestamp": true
  },
  "experimental": {
    "clash_api": {
      "external_controller": "127.0.0.1:9090",
      "external_ui": "ui",
      "secret": "",
      "external_ui_download_url": "https://gh-proxy.com/https://github.com/MetaCubeX/metacubexd/archive/refs/heads/gh-pages.zip",
      "external_ui_download_detour": "direct-out",
      "default_mode": "rule"
    },
    "cache_file": {
      "enabled": true,
      "store_rdrc": true
    }
  },
  "dns": {
    "servers": [
      {
        "tag": "cf-dns",
        "type": "tls",
        "server": "1.1.1.1",
        "detour": "proxy-out"
      },
      {
        "tag": "ali-dns",
        "type": "https",
        "server": "223.5.5.5"
      }
    ],
    "rules": [
      {
        "clash_mode": "direct",
        "server": "ali-dns"
      },
      {
        "clash_mode": "global",
        "server": "cf-dns"
      },
      {
        "rule_set": "manual-dns-proxy",
        "server": "cf-dns"
      },
      {
        "rule_set": "manual-dns-direct",
        "server": "ali-dns"
      },
      {
        "rule_set": [
          "geosite-cn",
          "geosite-geolocation-cn"
        ],
        "server": "ali-dns"
      },
      {
        "type": "logical",
        "mode": "and",
        "rules": [
          {
            "rule_set": [
              "geosite-cn",
              "geosite-geolocation-cn"
            ],
            "invert": true
          },
          {
            "ip_version": 6
          }
        ],
        "action": "predefined",
        "rcode": "NXDOMAIN"
      }
    ],
    "final": "cf-dns",
    "strategy": "prefer_ipv4"
  },
  "inbounds": [
    {
      "tag": "tun-in",
      "type": "tun",
      "address": [
        "172.19.0.0/30",
        "fdfe:dcba:9876::0/126"
      ],
      "stack": "system",
      "auto_route": true,
      "strict_route": true
    },
    {
      "tag": "mixed-in",
      "type": "mixed",
      "listen": "::",
      "listen_port": 7890
    }
  ],
  "outbounds": [
    {
      "tag": "proxy-out",
      "type": "selector",
      "outbounds": [
        "auto-out",
        "hk-out",
        "us-out",
        "{all}"
      ],
      "default": "auto-out"
    },
    {
      "tag": "Google",
      "type": "selector",
      "outbounds": [
        "proxy-out",
        "hk-out",
        "tw-out",
        "sg-out",
        "jp-out",
        "us-out",
        "others-out",
        "{all}"
      ],
      "default": "proxy-out"
    },
    {
      "tag": "AI",
      "type": "selector",
      "outbounds": [
        "proxy-out",
        "hk-out",
        "tw-out",
        "sg-out",
        "jp-out",
        "us-out",
        "others-out",
        "{all}"
      ],
      "default": "us-out"
    },
    {
      "tag": "Apple",
      "type": "selector",
      "outbounds": [
        "proxy-out",
        "direct-out",
        "hk-out",
        "tw-out",
        "sg-out",
        "jp-out",
        "us-out",
        "others-out"
      ],
      "default": "direct-out"
    },
    {
      "tag": "Microsoft",
      "type": "selector",
      "outbounds": [
        "proxy-out",
        "direct-out",
        "hk-out",
        "tw-out",
        "sg-out",
        "jp-out",
        "us-out",
        "others-out"
      ],
      "default": "direct-out"
    },
    {
      "tag": "Games",
      "type": "selector",
      "outbounds": [
        "proxy-out",
        "direct-out",
        "hk-out",
        "tw-out",
        "sg-out",
        "jp-out",
        "us-out",
        "others-out"
      ],
      "default": "direct-out"
    },
    {
      "tag": "Default",
      "type": "selector",
      "outbounds": [
        "proxy-out",
        "direct-out",
        "hk-out",
        "tw-out",
        "sg-out",
        "jp-out",
        "us-out",
        "others-out"
      ],
      "default": "proxy-out"
    },
    {
      "tag": "China",
      "type": "selector",
      "outbounds": [
        "direct-out",
        "hk-out",
        "tw-out",
        "sg-out",
        "jp-out",
        "us-out",
        "others-out"
      ],
      "default": "direct-out"
    },
    {
      "tag": "hk-out",
      "type": "urltest",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "include",
          "keywords": [
            "HK|hk|香港|HongKong"
          ]
        },
        {
          "action": "exclude",
          "keywords": [
            "海外"
          ]
        }
      ],
      "url": "http://www.gstatic.com/generate_204",
      "interval": "10m",
      "tolerance": 50
    },
    {
      "tag": "tw-out",
      "type": "urltest",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "include",
          "keywords": [
            "TW|tw|台湾|Taiwan"
          ]
        },
        {
          "action": "exclude",
          "keywords": [
            "海外"
          ]
        }
      ],
      "url": "http://www.gstatic.com/generate_204",
      "interval": "10m",
      "tolerance": 50
    },
    {
      "tag": "sg-out",
      "type": "urltest",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "include",
          "keywords": [
            "SG|sg|新加坡|Singapore"
          ]
        },
        {
          "action": "exclude",
          "keywords": [
            "海外"
          ]
        }
      ],
      "url": "http://www.gstatic.com/generate_204",
      "interval": "10m",
      "tolerance": 50
    },
    {
      "tag": "jp-out",
      "type": "urltest",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "include",
          "keywords": [
            "JP|jp|日本|Japan"
          ]
        },
        {
          "action": "exclude",
          "keywords": [
            "海外"
          ]
        }
      ],
      "url": "http://www.gstatic.com/generate_204",
      "interval": "10m",
      "tolerance": 50
    },
    {
      "tag": "us-out",
      "type": "urltest",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "include",
          "keywords": [
            "US|us|美国"
          ]
        },
        {
          "action": "exclude",
          "keywords": [
            "海外"
          ]
        }
      ],
      "url": "http://www.gstatic.com/generate_204",
      "interval": "10m",
      "tolerance": 50
    },
    {
      "tag": "others-out",
      "type": "selector",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "exclude",
          "keywords": [
            "HK|hk|香港|HongKong|TW|tw|台湾|Taiwan|SG|sg|新加坡|Singapore|JP|jp|日本|Japan|US|us|美国|海外"
          ]
        }
      ]
    },
    {
      "tag": "auto-out",
      "type": "urltest",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "exclude",
          "keywords": [
            "海外"
          ]
        }
      ],
      "url": "http://www.gstatic.com/generate_204",
      "interval": "10m",
      "tolerance": 50
    },
    {
      "tag": "direct-out",
      "type": "direct"
    }
  ],
  "route": {
    "default_domain_resolver": {
      "server": "ali-dns"
    },
    "auto_detect_interface": true,
    "final": "proxy-out",
    "rules": [
      {
        "inbound": [
          "tun-in"
        ],
        "action": "sniff"
      },
      {
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "port": 53
          },
          {
            "protocol": "dns"
          }
        ],
        "action": "hijack-dns"
      },
      {
        "protocol": "bittorrent",
        "outbound": "direct-out"
      },
      {
        "clash_mode": "direct",
        "outbound": "direct-out"
      },
      {
        "clash_mode": "global",
        "outbound": "proxy-out"
      },
      {
        "rule_set": [
          "manual-traffic-proxy",
          "geosite-github"
        ],
        "outbound": "proxy-out"
      },
      {
        "rule_set": "manual-traffic-direct",
        "outbound": "direct-out"
      },
      {
        "ip_is_private": true,
        "outbound": "direct-out"
      },
      {
        "rule_set": [
          "geosite-category-ai-!cn"
        ],
        "outbound": "AI"
      },
      {
        "rule_set": [
          "geosite-google",
          "geoip-google"
        ],
        "outbound": "Google"
      },
      {
        "rule_set": [
          "geoip-apple",
          "geosite-apple",
          "geosite-itunes"
        ],
        "outbound": "Apple"
      },
      {
        "rule_set": "geosite-microsoft",
        "outbound": "Microsoft"
      },
      {
        "rule_set": [
          "geosite-category-games"
        ],
        "outbound": "Games"
      },
      {
        "rule_set": "geosite-geolocation-!cn",
        "outbound": "Default"
      },
      {
        "rule_set": [
          "geoip-cn",
          "geosite-cn",
          "geosite-geolocation-cn"
        ],
        "outbound": "China"
      }
    ],
    "rule_set": [
      {
        "tag": "geosite-category-ai-!cn",
        "type": "remote",
        "format": "binary",
        "url": "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geosite/category-ai-!cn.srs",
        "download_detour": "direct-out"
      },
      {
        "tag": "geoip-google",
        "type": "remote",
        "format": "binary",
        "url": "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo-lite/geoip/google.srs",
        "download_detour": "direct-out"
      },
      {
        "tag": "geosite-google",
        "type": "remote",
        "format": "binary",
        "url": "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geosite/google.srs",
        "download_detour": "direct-out"
      },
      {
        "tag": "geoip-apple",
        "type": "remote",
        "format": "binary",
        "url": "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo-lite/geoip/apple.srs",
        "download_detour": "direct-out"
      },
      {
        "tag": "geosite-apple",
        "type": "remote",
        "format": "binary",
        "url": "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geosite/apple.srs",
        "download_detour": "direct-out"
      },
      {
        "tag": "geosite-itunes",
