    {
      "tag":"hk-out",
      "type":"urltest",
      "outbounds":[
        "{all}"
      ],
      "filter":[
        {"action":"include","keywords":["HK|hk|香港|HongKong"]},
		{"action":"exclude","keywords":["海外"]}
      ],
      "url": "http://www.gstatic.com/generate_204",
      "interval": "10m",
      "tolerance": 50
    },
    {
      "tag":"tw-out",
      "type":"urltest",
      "outbounds":[
        "{all}"
      ],
      "filter":[
        {"action":"include","keywords":["TW|tw|台湾|Taiwan"]},
		{"action":"exclude","keywords":["海外"]}
      ],
