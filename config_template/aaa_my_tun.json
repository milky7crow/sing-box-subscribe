    {
      "tag":"us-out",
      "type":"urltest",
      "outbounds":[
        "{all}"
      ],
      "filter":[
        {"action":"include","keywords":["US|us|美国"]},
		{"action":"exclude","keywords":["海外"]}
      ],
      "url": "http://www.gstatic.com/generate_204",
      "interval": "10m",
      "tolerance": 50
    },
    {
      "tag":"others-out",
      "type":"selector",
      "outbounds":[
        "{all}"
      ],
      "filter":[
        {"action":"exclude","keywords":["HK|hk|香港|HongKong|TW|tw|台湾|Taiwan|SG|sg|新加坡|Singapore|JP|jp|日本|Japan|US|us|美国|海外"]}
      ]
    },
    {
      "tag":"auto-out",
      "type":"urltest",
      "outbounds":[
        "{all}"
      ],
      "filter":[
        {"action":"exclude","keywords":["海外"]}
      ],
      "url": "http://www.gstatic.com/generate_204",
      "interval": "10m",
      "tolerance": 50
    },
    {
      "tag": "direct-out",
      "type": "direct"
    }
