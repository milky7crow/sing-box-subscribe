{
  "log": {
    "level": "warn",
    "timestamp": true
  },
  "experimental": {
    "clash_api": {
      "external_controller": "127.0.0.1:9090",
      "external_ui": "ui",
      "secret": "",
      "external_ui_download_url": "https://gh-proxy.com/https://github.com/MetaCubeX/metacubexd/archive/refs/heads/gh-pages.zip",
      "external_ui_download_detour": "direct-out",
      "default_mode": "rule"
    },
  "cache_file": {
    "enabled": true,
    "store_rdrc": true
    }
  },
  "dns": {
    "servers": [
      {
        "tag": "cf-dns",
        "type": "tls",
        "server": "1.1.1.1",
        "detour": "proxy-out"
      },
      {
        "tag": "ali-dns",
        "type": "https",
        "server": "223.5.5.5"
      }
    ],
    "rules": [
      {
        "clash_mode": "direct",
        "server": "ali-dns"
      },
      {
        "clash_mode": "global",
        "server": "cf-dns"
      },
      {
        "rule_set": "geosite-geolocation-!cn",
        "query_type": ["AAAA"],
        "action": "predefined",
        "rcode": "NOERROR",
        "answer": []
      },
      {
        "rule_set": "manual-dns-proxy",
        "server": "cf-dns"
      },
      {
        "rule_set": "manual-dns-direct",
        "server": "ali-dns"
      },
      {
        "rule_set": "geosite-cn",
        "server": "ali-dns"
      },
      {
        "type": "logical",
        "mode": "and",
        "rules": [
          {
            "rule_set": "geosite-geolocation-!cn",
            "invert": true
          },
          {
            "rule_set": "geoip-cn"
          }
        ],
        "server": "cf-dns",
        "client_subnet": "114.114.114.114/24"
      }
    ],
    "final": "cf-dns",
    "strategy": "prefer_ipv4"
  },
  "inbounds": [
    {
      "tag": "tun-in",
      "type": "tun",
      "address": [
        "172.19.0.0/30",
        "fdfe:dcba:9876::0/126"
      ],
      "stack": "system",
      "auto_route": true,
      "strict_route": true
    }
  ],
  "outbounds": [
    {
      "tag":"proxy-out",
      "type":"selector",
      "outbounds":[
        "auto-out",
        "hk-out",
        "us-out",
        "{all}"
      ],
      "default": "auto-out"
    },
    {
